print('gpc d2 tweedie score : ',d2_tweedie_score(ytest,gpc.predict(xtest)))
print('gpc explained variance score : ',explained_variance_score(ytest,gpc.predict(xtest)))
print('gpc fowlkes mallows score : ',fowlkes_mallows_score(ytest,gpc.predict(xtest)))
print('gpc fbeta score : ',fbeta_score(ytest,gpc.predict(xtest),beta=.5,average='weighted'))
print('gpc hamming loss : ',hamming_loss(ytest,gpc.predict(xtest)))
print('gpc homogeneity completeness v measure : ',homogeneity_completeness_v_measure(ytest,gpc.predict(xtest)))
print('gpc homogeneity score : ',homogeneity_score(ytest,gpc.predict(xtest)))
print('gpc jaccard score : ',jaccard_score(ytest,gpc.predict(xtest),average='weighted'))
