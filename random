import pyttsx3,pygame
class robot :
    engine = pyttsx3.init()
    def __init__(self,name):
        self.name = name
        self.battery = 100
        self.expression = 'neutral'
    def speak(self,text):
        self.engine.say(text)
        self.engine.runAndWait()
    def introduction(self):
        self.speak(f'hello i m {self.name} my current expression is {self.expression}')
    def changeexpression(self,newexpression):
        self.expression = newexpression
        self.speak(f"{self.name}'s expression is now {self.expression}")
    def play_sound(self,f):
        pygame.mixer.music.load(f)
        pygame.mixer.music.play()
    def alarm(self):
        self.speak('warning i m about to paly an alarm sound')
        pygame.mixer.music.load('warning.wav')
        pygame.mixer.music.play()
def main():
        my_robot = robot('jeriq')
        my_robot.introduction()
        my_robot.changeexpression('sad')
        my_robot.play_sound('dontLike.mp3')
        my_robot.alarm()
if __name__ == '__main__':main()
voices = engine.getProperty('voices')
engine.setProperty('voice', voices[0].id)
engine.say('Hello World')
engine.runAndWait()
import pygame
pygame.mixer.init()
pygame.mixer.music.load('dontLike.mp3')
pygame.mixer.music.play()
while pygame.mixer.music.get_busy():
    pygame.time.Clock().tick(10)
import importlib.metadata
installed_packages = importlib.metadata.distributions()
for dist in installed_packages:
    print(f"{dist.metadata['Name']}=={dist.version}")
