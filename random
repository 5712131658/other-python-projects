import numpy,tensorflow
from tensorflow.keras.preprocessing import image
def predict_and_detect_class(model, img_path, target_class, img_size=(100, 100)):
    img = image.load_img(img_path, target_size=img_size)
    img_array = image.img_to_array(img)
    img_array = numpy.expand_dims(img_array, axis=0)
    img_array = tensorflow.keras.applications.resnet.preprocess_input(img_array)
    predictions = model.predict(img_array)
    predicted_class = numpy.argmax(predictions)
    confidence_score = predictions[0][predicted_class]
    if predicted_class == target_class:
        print(f"Detected the target class : Class {target_class} and score is {confidence_score}!")
    else:
        print(f"Predicted Class : {predicted_class} - Not the target class and score is {confidence_score}.")
    return predicted_class, predictions
