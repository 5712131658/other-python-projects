import matplotlib.pyplot,numpy
x = numpy.linspace(0,10,100)
y = numpy.sin(x)
y1 = numpy.cos(x)
y2 = numpy.tan(x)
y3 = numpy.tanh(x)
y4 = numpy.sinc(x)
y5 = numpy.sinh(x)
y6 = numpy.cosh(x)
fig ,(ax1,ax2,ax3,ax4,ax5,ax6,ax7) = matplotlib.pyplot.subplots(1,7,figsize=(50,50))
ax1.plot(x,y)
ax2.plot(x,y1)
ax3.plot(x,y2)
ax4.plot(x,y3)
ax5.plot(x,y4)
ax6.plot(x,y5)
ax7.plot(x,y6)
matplotlib.pyplot.show()
