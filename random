import matplotlib.pyplot,numpy
x = numpy.linspace(0,10,100)
y = numpy.sin(x)
y1 = numpy.cos(x)
y2 = numpy.tan(x)
y3 = numpy.tanh(x)
y4 = numpy.sinc(x)
y5 = numpy.sinh(x)
y6 = numpy.cosh(x)
fig ,(ax1,ax2,ax3,ax4,ax5,ax6,ax7) = matplotlib.pyplot.subplots(1,7,figsize=(50,50))
ax1.plot(x,y)
ax2.plot(x,y1)
ax3.plot(x,y2)
ax4.plot(x,y3)
ax5.plot(x,y4)
ax6.plot(x,y5)
ax7.plot(x,y6)
matplotlib.pyplot.show()
X1 = [4,1,5,7]
print(X1)
X1[2] = 10
print(X1)
X1 = [i + 2 for i in X1]
print(X1)
dataset = [2, 1, 1, 4, 5, 8, 12, 4, 3, 8, 21, 1, 18, 5]
number_of_items = len(dataset)
print(number_of_items)
sum_of_items = sum(dataset)
print(sum_of_items)
mean = sum_of_items/number_of_items
print(mean)
dataset = sorted(dataset)
print(dataset)
middle_index = len(dataset)//2
print (middle_index)
median = dataset[middle_index] + dataset[middle_index-1]
median = median/2
print (median)
mode = max(dataset,key=dataset.count)
print (mode)
