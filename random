y = dataframe.mean_temperature
fig, (ax1, ax2,ax3,ax4) = matplotlib.pyplot.subplots(1, 4,  sharey=True) #create 2 subplots with shared y axis
fig.suptitle('Sharing Y axis')
ax1.plot(x, y)
ax2.scatter(x, y)
ax3.boxplot(y)
ax4.hist(y)
